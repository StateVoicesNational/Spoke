{
  "presets": ["react", "env", "stage-0"],
  "only": ["*.js", "*.jsx"],
  "plugins": [
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }]
  ],
  "env": {
    "dev": {
      "plugins":["react-hot-loader/babel"]
    },
    "development": {
      "plugins":["react-hot-loader/babel"]
    }
  }
}

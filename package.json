{
  "name": "spoke",
  "version": "1.0.0",
  "description": "Spoke",
  "main": "src/server",
  "engines": {
    "node": "6.2.2",
    "npm": "3.7.3"
  },
  "scripts": {
    "test": "npm run lint",
    "clean": "rm -rf $OUTPUT_DIR",
    "lint": "eslint --fix --ext js --ext jsx src",
    "prod-build-client": "webpack --config ./webpack/config.js",
    "prod-build-server": "babel ./src -d $OUTPUT_DIR/server --source-maps",
    "prod-build": "npm run clean && npm run prod-build-client && npm run prod-build-server",
    "postinstall": "if [ \"$NODE_ENV\" = production ] ; then npm run prod-build ; fi",
    "start": "node $OUTPUT_DIR/server/server",
    "predev-start": "npm run clean",
    "dev-start": "nodemon -e js,jsx,md,mustache -w ./src --exec ./dev-tools/babel-run -- ./src/server",
    "generate-contacts": "./dev-tools/babel-run ./dev-tools/generate-contacts.js",
    "dev": "nf start -w --procfile ./dev-tools/Procfile.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saikat/spoke.git"
  },
  "keywords": [
    "spoke",
    "sms",
    "texting",
    "campaigns",
    "campaigning",
    "politics",
    "activism",
    "organizing",
    "field"
  ],
  "author": "Axle Factory",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/saikat/spoke/issues"
  },
  "homepage": "https://github.com/saikat/saikat/spoke/#readme",
  "dependencies": {
    "aphrodite": "^0.4.1",
    "apollo-client": "^0.4.7",
    "apollo-server": "^0.1.5",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babyparse": "^0.4.6",
    "body-parser": "^1.15.2",
    "bonzo": "^2.0.0",
    "chart.js": "^1.1.1",
    "checksum": "^0.1.1",
    "cookie-session": "^2.0.0-alpha.1",
    "dataloader": "^1.2.0",
    "draft-js": "^0.7.0",
    "express": "^4.14.0",
    "google-libphonenumber": "^1.0.25",
    "graphql": "^0.6.1",
    "graphql-tag": "^0.1.9",
    "graphql-type-json": "^0.1.2",
    "humps": "^1.1.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.13.1",
    "mailgun-js": "^0.7.11",
    "material-ui": "^0.15.2",
    "minilog": "^3.0.1",
    "moment": "^2.14.1",
    "nexmo": "^1.0.0-beta-7",
    "papaparse": "^4.1.2",
    "passport": "^0.3.2",
    "passport-auth0": "^0.5.2",
    "react": "^15.2.1",
    "react-addons-css-transition-group": "^15.2.1",
    "react-apollo": "^0.3.15",
    "react-async-script": "^0.6.0",
    "react-chartjs": "^0.7.3",
    "react-dom": "^15.2.1",
    "react-formal": "^0.18.5",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "react-tap-event-plugin": "^1.0.0",
    "redux-thunk": "^2.1.0",
    "rollbar": "^0.6.2",
    "stripe": "^4.9.0",
    "thinky": "^2.3.3",
    "transform-loader": "^0.2.3",

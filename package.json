  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-register": "^6.9.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "faker": "^3.1.0",
    "foreman": "^1.4.1",
    "json2csv": "^3.6.2",
    "nodemon": "^1.9.2",
    "react-hot-loader": "^1.3.0",
    "webpack-dev-server": "^1.14.1"
  }
}

{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": ["error", "never"],
    "jsx-quotes": ["error", "prefer-single"],
    "max-len": ["off"],
    "no-alert": ["off"],
    "no-console": ["off"],
    "prefer-template": ["off"],
    "semi": ["error", "never"],
    "react/require-extension": "off",
  },
  "env": { "jest": true, "node": true, "browser": true }
}
